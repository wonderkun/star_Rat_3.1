    BEGIN
        MENUITEM "Maþos piktogramos",                XTP_IDC_SMALLICON
        MENUITEM "Didelës piktogramos",                XTP_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "Anuliuoti",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cut",                        ID_EDIT_CUT
        MENUITEM "Copy",                       ID_EDIT_COPY
