        BEGIN
            MENUITEM "Kairioji lygiuotë",                  XTP_IDC_ALIGNLEFT
            MENUITEM "Centravimas",                XTP_IDC_ALIGNCENTER
            MENUITEM "Deðinioji lygiuotë",                 XTP_IDC_ALIGNRIGHT
        END
    END
END

