XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Âtrâs piekïuves"
    BEGIN
        MENUITEM "Noòemt no âtrâs piekïuves rîkjoslas",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Pievienot âtrâs piekïuves rîkjoslai",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Pielâgot âtrâs piekïuves rîkjoslai ...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Râdît âtrâs piekïuves rîkjoslu zem lentes",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Parâdît âtrâs piekïuves rîkjoslai Virs lentç",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Minimizçt lenti",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Âtrâs piekïuves"
    BEGIN
        MENUITEM "Âtrâs piekïuves rîkjoslas pielâgoðana",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
