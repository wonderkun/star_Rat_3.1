        MENUITEM SEPARATOR
        MENUITEM "Vairâk komandas...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Râdît zem lentes",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Râdît Virs lentç",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Minimizçt lenti",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

