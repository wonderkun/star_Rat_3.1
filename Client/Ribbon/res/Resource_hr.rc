XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Brzi pristup"
    BEGIN
        MENUITEM "Ukloni s alatne trake za brzi pristup",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Dodaj u alatnu traku za brzi pristup",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Prilagodba alatne trake za brzi pristup ...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Prikaži alatnu traku za brzi pristup ispod vrpci",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Prikaži alatnu traku za brzi pristup iznad vrpce",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Minimiziraj vrpcu",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Brzi pristup"
    BEGIN
        MENUITEM "Prilagodba alatne trake za brzi pristup",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Naredbe]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Više Naredbe...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Prikaži Ispod vrpci",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Prikaži Iznad vrpci",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Minimiziraj vrpcu",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

