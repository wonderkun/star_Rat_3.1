XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Accès rapide"
    BEGIN
        MENUITEM "Supp&rimer de la barre d'outils Accès rapide",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Ajouter à la barre d'outils Accès rapide",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Personnaliser la barre d'outils A&ccès rapide...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Afficher la barre d'outils Acc&ès rapide sous le ruban",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Afficher la barre d'outils Accès ra&pide au-dessus du ruban",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Réduire le ruba&n",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Accès rapide"
    BEGIN
        MENUITEM "Personnaliser la barre d'outils Accès rapide",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Commandes]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Autres commandes...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Afficher en dessous du ruban",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Afficher au-dessus du ruban",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Réduire le ruba&n",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

