XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Acceso rápido"
    BEGIN
        MENUITEM "Eliminar de la barra de herramientas de acceso rápido",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Agregar a la barra de herramientas de acceso rápido",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Personalizar barra de herramientas de a&cceso rápido...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Mostrar la barra de herramientas de acceso rápido por debajo de la cinta de opciones",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Mostrar la barra de herramientas de acceso rápido por encima de la cinta de opciones",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Minimizar la cinta de opciones",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Acceso rápido"
    BEGIN
        MENUITEM "Personalizar barra de herramientas de acceso rápido",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Comandos]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Más comandos...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Mostrar debajo de la cinta de opciones",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Mostrar encima de la cinta de opciones",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Minimizar la cinta de opciones",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

