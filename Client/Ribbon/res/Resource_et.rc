XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Kiireks juurdepääsuks"
    BEGIN
        MENUITEM "Digitaalserdi hankimine eemaldamine",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Lisamine kiirpääsuribale",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Kohanda kiirpääsuriba...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Kuva kiirpääsuriba lindi all",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Kuva kiirpääsuriba lindi kohal",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Lindi kasutamine",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Kiireks juurdepääsuks"
    BEGIN
        MENUITEM "Kohanda kiirpääsuriba",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Käsud]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Rohkem käske...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Kuva lindi all",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Kuva lindi kohal",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Lindi kasutamine",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

