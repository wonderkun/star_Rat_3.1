XTP_IDR_RIBBONCUSTOMIZEMENU MENU DISCARDABLE 
BEGIN
    POPUP "Rask tilgang"
    BEGIN
        MENUITEM "Fjern fra verktøylinjen for hurtigtilgang",               XTP_ID_RIBBONCUSTOMIZE_REMOVE
        MENUITEM "Legg til rask tilgang til verktøylinjen",                    XTP_ID_RIBBONCUSTOMIZE_ADD
        MENUITEM SEPARATOR
        MENUITEM "Tilpasse rask tilgang til verktøylinjen...",              XTP_ID_RIBBONCUSTOMIZE
        MENUITEM "Vis rask tilgang til verktøylinjen nedenfor båndet",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW
        MENUITEM "Vis rask tilgang til verktøylinjen over fargebånd",     XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE
        MENUITEM SEPARATOR
        MENUITEM "Minimere fargebånd",              XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

XTP_IDR_RIBBONCUSTOMIZEMENUMORE MENU DISCARDABLE 
BEGIN
    POPUP "Rask tilgang"
    BEGIN
        MENUITEM "Tilpasse rask tilgang til verktøylinjen",                 XTP_ID_RIBBONCUSTOMIZE_LABEL
        MENUITEM "[Kommandoer]",                                      XTP_ID_RIBBONCUSTOMIZE_COMMANDS
        MENUITEM SEPARATOR
        MENUITEM "Flere kommandoer...",                               XTP_ID_RIBBONCUSTOMIZE_MORE
        MENUITEM "Vis under båndet",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSBELOW_MORE
        MENUITEM "Vis over båndet",                          XTP_ID_RIBBONCUSTOMIZE_QUICKACCESSABOVE_MORE
        MENUITEM SEPARATOR
        MENUITEM "Minimere fargebånd",                            XTP_ID_RIBBONCUSTOMIZE_MINIMIZE
    END
END

